{
  "private": true,
  "scripts": {
    "analyze-dev": "npm run dev -- --analyze",
    "analyze-prod": "npm run dev -- --analyze",
    "inspect-dev": "npm run dev -- --inspect",
    "inspect-prod": "npm run dev -- --inspect",
    "dev-modern": "npm run dev -- --modern",
    "prod-modern": "npm run prod -- --modern",
    "dev": "cross-env TS_NODE_PROJECT=\"node_modules/varie-bundler/webpack-tsconfig.json\" webpack --env development",
    "prod": "cross-env TS_NODE_PROJECT=\"node_modules/varie-bundler/webpack-tsconfig.json\" webpack --env production",
    "watch": "cross-env TS_NODE_PROJECT=\"node_modules/varie-bundler/webpack-tsconfig.json\" webpack-dev-server --mode development --progress --hot"
  },
  "dependencies": {
    "varie": "^1.0.1",
    "vue": "^2.6.0",
    "vue-router": "^3.0.0",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^8.0.3",
    "varie-bundler": "^3.0.4-rc4",
    "vue-template-compiler": "^2.6.0"
  }
}
